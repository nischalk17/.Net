using System;

// Custom attribute with positional and named parameters

[AttributeUsage(AttributeTargets.Class | AttributeTargets.Method)]
public class MyCustomAttribute : Attribute
{
    public string Name { get; }
    public int Id { get; }
    public string Description { get; set; } // Named parameter

    // Constructor with positional parameters
    public MyCustomAttribute(string name, int id)
    {
        Name = name;
        Id = id;
    }
}

[MyCustomAttribute("SampleClass", 1, Description = "This is a sample class")] // Positional and named parameters
class SampleClass
{
    [MyCustomAttribute("SampleMethod", 2, Description = "This is a sample method")] // Positional and named parameters
    public void SampleMethod()
    {
        Console.WriteLine("Method executed");
    }
}

class Program
{
    static void Main()
    {
        // Create an object of SampleClass
        SampleClass obj = new SampleClass();
        obj.SampleMethod();
    }
}
