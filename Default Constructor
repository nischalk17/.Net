using System;

class Car
{
    string brand;

    // Default Constructor (No parameters)
    public Car()
    {
        brand = "Toyota";
    }

    public void Display()
    {
        Console.WriteLine("Car Brand: " + brand);
    }

    static void Main()
    {
        Car myCar = new Car();  // Constructor is called automatically
        myCar.Display();
    }
}


/* Output:
Car Brand: Toyota
*/
